<%= form_for @event do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>

  <%= f.label :location %><br>
  <%= f.collection_select :location_id, Location.all, :id, :name%><br>

  <%= f.label :category %><br>
  <%= f.collection_select :category_id, Category.all, :id, :name %><br>

  <%= f.label :start_time %><br>
  <%= f.datetime_select :start_time %><br>

  <%= f.label :end_time %><br>
  <%= f.datetime_select :end_time %><br>


  <%= f.submit %>
  <% end %>
